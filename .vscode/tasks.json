{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "swift",
			"args": [
				"build",
				"--build-tests",
				"-Xswiftc",
				"-diagnostic-style=llvm"
			],
			"env": {},
			"cwd": "/media/psf/SwiftSDL",
			"disableTaskQueue": true,
			"showBuildStatus": "swiftStatus",
			"group": "build",
			"problemMatcher": [],
			"label": "swift: Build All",
			"detail": "swift build --build-tests -Xswiftc -diagnostic-style=llvm"
		},
		{
			"type": "swift",
			"args": [
				"test",
				"--filter",
				"SwiftSDLTests.testCamera()"
			],
			"env": {},
			"cwd": "/media/psf/SwiftSDL",
			"disableTaskQueue": true,
			"showBuildStatus": "swiftStatus",
			"group": "test",
			"problemMatcher": [],
			"label": "swift: Test Camera App",
			"detail": "swift test --filter \"SwiftSDLTests.testCamera()\""
		}
	]
}